+  ConstraintMintDecimals = 2018,
+  ConstraintSpace = 2019,
+
+  // Require.
+  RequireViolated = 2500,
+  RequireEqViolated = 2501,
+  RequireKeysEqViolated = 2502,
+  RequireNeqViolated = 2503,
+  RequireKeysNeqViolated = 2504,
